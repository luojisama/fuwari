{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "shiro.team"
        }
      ],
      "destination": "https://blog.shiro.team/$1",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "www.shiro.team"
        }
      ],
      "destination": "https://blog.shiro.team/$1",
      "permanent": true
    }
  ]
}
